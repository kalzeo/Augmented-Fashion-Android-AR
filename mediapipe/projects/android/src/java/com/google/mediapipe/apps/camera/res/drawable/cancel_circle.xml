<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:color="#FFFFFF"
                android:width="3dp"/>
            <size
                android:height="80dp"
                android:width="80dp" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:color="#FFFFFF"
                    android:width="3dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
